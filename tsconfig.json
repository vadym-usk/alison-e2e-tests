{
    "compilerOptions": {
        "baseUrl": ".",
        "target": "ES2020",
        "lib": [
            "ES2020",
            "DOM"
        ],
        "module": "commonjs",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "types": [
            "cypress"
        ],
        "resolveJsonModule": true
    },
    "include": [
        "cypress/**/*.ts",
        "cypress.config.ts"
    ]
}
